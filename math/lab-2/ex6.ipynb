   "source": [
    "from sympy import beta, gamma\n",
    "m = float(input(\"m:\"))\n",
    "n = float(input(\"n:\"))\n",
    "\n",
    "s = beta(m,n)\n",
    "t = (gamma(m) * gamma(n)) / gamma(m+n)\n",
    "\n",
    "print(s,t)\n",
    "\n",
    "if(abs(s-t) <= 0.00001):\n",
    "    print(\"Beta and Gamma are Related\")\n",
    "else:\n",
    "    print(\"Invalid Input\")\n"
   ]
  }
