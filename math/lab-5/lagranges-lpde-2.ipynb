      "  ∂               ∂              \n",
      "2⋅──(F(x, y)) + 3⋅──(F(x, y)) = 1\n",
      "  ∂x              ∂y             \n",
      "\n",
      "\n",
      "          2⋅x   3⋅y               \n",
      "F(x, y) = ─── + ─── + F(3⋅x - 2⋅y)\n",
      "          13    13                \n"
     ]
    }
   ],
   "source": [
    "from sympy import *\n",
    "from sympy.solvers.pde import pdsolve\n",
    "x, y, a = symbols('x y a')\n",
    "F = Function('F')\n",
    "z = F(x,y)\n",
    "zx = z.diff(x)\n",
    "zy = z.diff(y)\n",
    "\n",
    "equation = Eq(2*zx + 3*zy, 1)\n",
    "\n",
    "pprint(equation)\n",
    "print(\"\\n\")\n",
    "sol = pdsolve(equation, z)\n",
    "pprint(sol)"
   ]
  }
 ],
