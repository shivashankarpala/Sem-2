   "source": [
    "from sympy import *\n",
    "from sympy.vector import *\n",
    "\n",
    "N = CoordSys3D('N')\n",
    "x,y,z = symbols('x y z')\n",
    "\n",
    "A = N.x**2 * N.y + 2 * N.x * N.z - 4\n",
    "\n",
    "delop = Del()\n",
    "gradA = gradient(A)\n",
    "\n",
    "display(delop(A))\n",
    "print(\"\\nGradient of \", A,\" is \\n\")\n",
    "display(gradA)"
   ]
