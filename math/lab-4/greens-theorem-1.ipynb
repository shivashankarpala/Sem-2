   "source": [
    "from sympy import *\n",
    "var('x,y')\n",
    "\n",
    "p = x + 2*y \n",
    "q = x - 2*y\n",
    "F = diff(q, x) - diff(p, y)\n",
    "sol = integrate(F, [x,0,1], [y,0,1])\n",
    "print(\"I = \", sol)"
   ]
  }
